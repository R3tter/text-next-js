{
  "name": "test-next-js",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "api=local next dev -p 8080",
    "build": "api=dev next build",
    "start": "next start"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/config/setupTests.js"
    ],
    "transform": {
      "^.+\\.(jsx|js)$": "babel-jest",
      "^.+\\.svg$": "jest-svg-transformer",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/fileTransformer.js"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      ".+\\.(scss|css)$": "identity-obj-paroxy"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/cypress/"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "@storybook/addon-actions": "^6.1.6",
    "@storybook/addon-essentials": "^6.1.6",
    "@storybook/addon-links": "^6.1.6",
    "@storybook/react": "^6.1.6",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/react": "^11.2.2",
    "@testing-library/react-hooks": "^3.4.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "css-loader": "^5.0.1",
    "cypress": "^6.0.0",
    "eslint": "^7.14.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-svg-transformer": "^1.0.0",
    "prettier": "^2.2.0",
    "prettier-eslint-cli": "^5.0.0",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0"
  },
  "dependencies": {
    "@jmind.systems/react-modal": "0.0.27",
    "@jmind.systems/react-status-controller": "0.0.14",
    "aphrodite": "^2.4.0",
    "api-req-redux": "^2.0.10",
    "core-js": "^3.8.3",
    "i18next": "^19.8.4",
    "next": "10.0.5",
    "prop-types": "^15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-i18next": "^11.8.5",
    "react-notify-library": "0.0.14",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "use-form-validation-hook": "^1.0.17"
  }
}
